[package]
name = "openrouter-sdk"
version = "0.1.4"
edition = "2021"
description = "Rust SDK for OpenRouter API"
license = "MIT"
repository = "https://github.com/zaoinc/openrouter-sdk"

[dependencies]
reqwest = { version = "0.11", features = ["json"] }
tokio = { version = "1.0", features = ["full"] }
serde = { version = "1.0", features = ["derive"] }
serde_json = "1.0"
thiserror = "1.0"